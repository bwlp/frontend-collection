{
  "name": "svelte-multiple-choice",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port=2997",
    "build": "vite build",
    "serve": "vite serve",
    "deploy": "vite build && npx gh-pages -d dist",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "outdated": "ncu -t minor -u",
    "outdated:update": "ncu -t minor -u; yarn upgrade; rm yarn.lock; yarn install --network-timeout 1000000",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "7.0.0-beta.42",
    "@storybook/addon-essentials": "^7.0.0-beta.42",
    "@storybook/addon-interactions": "^7.0.0-beta.42",
    "@storybook/addon-links": "^7.0.0-beta.42",
    "@storybook/blocks": "^7.0.0-beta.42",
    "@storybook/svelte": "^7.0.0-beta.42",
    "@storybook/svelte-vite": "^7.0.0-beta.42",
    "@storybook/testing-library": "^0.0.14-next.1",
    "@sveltejs/vite-plugin-svelte": "^2.0.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/svelte": "^3.2.2",
    "@testing-library/user-event": "^14.4.3",
    "@tsconfig/svelte": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "@vitest/coverage-c8": "^0.28.4",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-svelte": "^2.15.0",
    "gh-pages": "^5.0.0",
    "jsdom": "^21.1.0",
    "npm-check-updates": "^16.6.3",
    "postcss": "^8.4.21",
    "prettier": "^2.8.3",
    "prettier-plugin-svelte": "^2.9.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.0.0-beta.42",
    "svelte": "^3.55.1",
    "svelte-check": "^2.10.3",
    "tailwindcss": "^3.2.4",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5",
    "vite": "^4.0.4",
    "vitest": "^0.28.4"
  },
  "dependencies": {
    "carbon-icons-svelte": "^11.4.0"
  }
}
