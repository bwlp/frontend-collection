{
  "homepage": "https://bwlp.github.io/kana-practice",
  "name": "kana-practice",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/node": "^16.18.11",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "dev": "vite --port 2999",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "outdated": "ncu -t minor -u",
    "outdated:update": "ncu -t minor -u; yarn upgrade; rm yarn.lock; yarn install"
  },
  "eslintConfig": {},
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@vitejs/plugin-react": "^3.0.1",
    "@vitest/coverage-c8": "^0.27.1",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "gh-pages": "^3.2.3",
    "jsdom": "^21.0.0",
    "npm-check-updates": "^12.5.12",
    "prettier": "^2.8.2",
    "vitest": "^0.27.1"
  }
}
