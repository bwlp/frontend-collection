{
  "name": "multiple-choice",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port=3003",
    "build": "vite build",
    "serve": "vite serve",
    "deploy": "vite build && npx gh-pages -d dist",
    "lint": "eslint . --ext .js,.cjs,.ts,.json",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "outdated": "ncu -t minor -u",
    "outdated:update": "ncu -t minor -u; pnpm upgrade; rm pnpm-lock.yaml; pnpm install",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "7.6.7",
    "@storybook/addon-essentials": "7.6.7",
    "@storybook/addon-interactions": "7.6.7",
    "@storybook/addon-links": "7.6.7",
    "@storybook/blocks": "7.6.7",
    "@storybook/svelte": "7.6.7",
    "@storybook/svelte-vite": "7.6.7",
    "@storybook/testing-library": "^0.2.2",
    "@sveltejs/vite-plugin-svelte": "^2.5.3",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/svelte": "^3.2.2",
    "@testing-library/user-event": "^14.5.2",
    "@tsconfig/svelte": "^3.0.0",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitest/coverage-v8": "^0.34.6",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-svelte": "^2.35.1",
    "gh-pages": "^5.0.0",
    "jsdom": "^21.1.2",
    "npm-check-updates": "^16.14.12",
    "postcss": "^8.4.33",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "7.6.7",
    "svelte": "^3.59.2",
    "svelte-check": "^2.10.3",
    "tailwindcss": "^3.4.1",
    "tslib": "^2.6.2",
    "typescript": "^4.9.5",
    "vite": "^4.5.1",
    "vitest": "^0.34.6"
  },
  "dependencies": {
    "carbon-icons-svelte": "^11.4.0"
  }
}
